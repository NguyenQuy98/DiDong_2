<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:width="140dp"
        android:height="140dp">
        <shape

            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#DDDDDD"
                android:endColor="#33FFFF">
            </gradient>

        </shape>
    </item>

    <item
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <shape
            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="@color/endblue"
                android:endColor="#00575757">
            </gradient>

        </shape>
    </item>
    <item
        android:left="3dp"
        android:right="3dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape
            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="@color/startblue"
                android:endColor="#00141314">
            </gradient>

        </shape>
    </item>
    <item android:width="105dp"
        android:height="105dp"
        android:gravity="center"
        >
        <bitmap android:src="@drawable/iconlogo"></bitmap>

    </item>
</layer-list>